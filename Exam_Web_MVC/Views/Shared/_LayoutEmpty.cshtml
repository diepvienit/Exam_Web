
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta name="description" content="" />
    <meta name="author" content="" />
    <title>@ViewBag.Title</title>

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/Content/Plugins/DatetimePicker/bootstrap-datetimepicker.min.css" rel="stylesheet" media="screen" />
    <link href="~/Assets/Admin/css/styles.css" rel="stylesheet" />

    <script src="~/Scripts/jquery-3.5.1.min.js"></script>
    <script src="~/Content/Plugins/bootstrap-3.1.1-dist/js/bootstrap.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.1/js/all.min.js" crossorigin="anonymous"></script>
    <script src="~/Content/Plugins/DatetimePicker/bootstrap-datetimepicker.min.js" charset="UTF-8"></script>
    <script src="~/Content/Plugins/DatetimePicker/locales/bootstrap-datetimepicker.fr.js" charset="UTF-8"></script>
</head>
<body>
    <div>
        <div class="container">
            @RenderBody()
        </div>
    </div>
    <script src="~/Assets/Admin/scripts/scripts.js"></script>
</body>
</html>
